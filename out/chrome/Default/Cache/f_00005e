    function SmdPaginationModel(page, size) {
        this.page = page;
        this.size = size;
    }
    SmdPaginationModel.prototype.isInsidePage = function (index) {
        var end = (this.page * this.size) - 1;
        var begin = end - this.size + 1;
        return index >= begin && index <= end;
    };
    return SmdPaginationModel;
}());

var SmdPaginatorComponent = /** @class */ (function () {
    function SmdPaginatorComponent() {
        this.selectedPage = 1;
        this.count = 0;
