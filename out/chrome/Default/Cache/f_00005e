/**
 * Main Datatable Component
 */
var SmdDataTable = /** @class */ (function () {
    function SmdDataTable(differs, _viewContainer, changeDetector) {
        this._viewContainer = _viewContainer;
        this.changeDetector = changeDetector;
        this.rows = [];
        this.visibleRows = [];
        this.models = [];
        this.checked = false;
        this.paginated = true;
        this.paginatorRanges = [5, 10, 25, 50, 100];
        this.responsive = false;
        this.counts = 0;
        this.onRowSelected = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.onAllRowsSelected = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.pageChange = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();
        this.findLevel = 0;
        this.differ = differs.find([]).create(null);
    }
    Object.defineProperty(SmdDataTable.prototype, "rowCount", {
        get: function () {
            return this.counts;
        },
        enumerable: true,
        configurable: true
    });
    SmdDataTable.prototype.applyFilter = function (filterValue) {
        filterValue = filterValue.trim(); // Remove whitespace
        filterValue = filterValue.toLowerCase(); // MatTableDataSource defaults to lowercase matches
    };
    SmdDataTable.prototype.ngAfterContentInit = function () {
        var _this = this;
        this._updateRows();
        this._columnsSubscription = this.columns.changes.subscribe(function () {
            _this._updateRows();
            _this.changeDetector.markForCheck();
        });
    };
    SmdDataTable.prototype.ngDoCheck = function () {
        var changes = this.differ.diff(this.models);
        if (changes) {
            if (this.columns) {
                this._updateRows();
            }
        }
    };
    SmdDataTable.prototype.ngOnDestroy = function () {
        this._columnsSubscription.unsubscribe();
    };
    SmdDataTable.prototype._updateRows = function () {
        var _this = this;
        if (this.models) {
            this.rows.length = 0;
            this.findLevel = 0;
            this._loopModels(this.models);
            this.rows = this.rows.filter(function (row) { return _this._matches(row, _this.columns.toArray(), _this.header.filterValue); });
            this.rows.forEach(function (row, index) { return row.originalOrder = index; });
            this._updateVisibleRows();
        }
    };
    SmdDataTable.prototype._loopModels = function (models, level) {
        if (level === void 0) { level = 0; }
        for (var i = 0; i < models.length; i++) {
            var data = new SmdDataRowModel(models[i], this.checked, this.findLevel);
            this.rows.push(data);
            if (models[i].children && models[i].children.length) {
                this.findLevel = this.findLevel + 1;
                this._loopModels(models[i].children, this.findLevel);
            }
            if (i == models.length - 1) {
                this.findLevel = (level - 1);
            }
        }
        /*this.models.forEach((model: any, index: number) => {
         this.rows[index] = new SmdDataRowModel(model, this.checked);

         });*/
    };
    SmdDataTable.prototype._matches = function (row, columns, text) {
        if (typeof (text) == "undefined" || text == null || text.trim() == '') {
            return true;
        }
        var subtexts = text.trim().split(" ");
        for (var _i = 0, subtexts_1 = subtexts; _i < subtexts_1.length; _i++) {
            var subtext = subtexts_1[_i];
            for (var _a = 0, columns_1 = columns; _a < columns_1.length; _a++) {
                var column = columns_1[_a];
                var filterFn = this._filterValue;
                var value = column.getFieldValue(row.model);
                if (column.hasCustomTemplate) {
                    value = row.model;
                    filterFn = column.filterFn ? column.filterFn : function (value, text) { return false; };
                }
                if (filterFn(value, subtext)) {
                    return true;
                }
            }
        }
        return false;
    };
    SmdDataTable.prototype._filterValue = function (value, text) {
        return value && value.toString().toUpperCase().indexOf(text.toString().toUpperCase()) > -1;
    };
    SmdDataTable.prototype.selectedRows = function () {
        return this.rows.filter(function (row) { return row.checked; });
    };
    SmdDataTable.prototype.selectedModels = function () {
        return this.selectedRows().map(function (row) { return row.model; });
    };
    SmdDataTable.prototype._onMasterCheckChange = function () {
        var _this = this;
        this.rows
            .forEach(function (row) {
            if (row.checked != _this.checked) {
                row.checked = _this.checked;
            }
        });
        this.onAllRowsSelected.emit(this.checked);
    };
    SmdDataTable.prototype._onRowCheckChange = function (row) {
        var isMasterChecked = this.checked;
        if (row.checked) {
            if (this.rows.filter(function (row) { return row.checked; }).length == this.rows.length) {
                this.checked = true;
            }
        }
        else {
            if (this.checked) {
                this.checked = false;
            }
        }
        this.onRowSelected.emit({
            model: row.model,
            checked: row.checked
        });
        if (this.checked != isMasterChecked) {
            this.onAllRowsSelected.emit(this.checked);
        }
    };
    SmdDataTable.prototype._onFilter = function (event) {
        this.paginatorComponent.reset();
        this._updateRows();
    };
    SmdDataTable.prototype._sortColumn = function (column) {
        var _this = this;
        if (column.sortable) {
            this.columns.filter(function (col) { return col.id != column.id; }).forEach(function (col) { return col.sortDir = null; });
            if (!column.sortDir) {
                column.sortDir = 'asc';
            }
            else {
                column.sortDir = column.sortDir == 'asc' ? 'desc' : null;
            }
            if (column.sortDir != null) {
                this.rows.sort(function (itemA, itemB) {
                    var sortFn = column.sortFn ? column.sortFn : _this._sortRows;
                    var a = itemA.model;
                    var b = itemB.model;
                    if (!column.sortFn) {
                        a = column.getFieldValue(itemA.model);
                        b = column.getFieldValue(itemB.model);
                    }
                    return sortFn(a, b, column.sortDir);
                });
            }
            else {
                this.rows.sort(function (itemA, itemB) {
                    return _this._sortRows(itemA.originalOrder, itemB.originalOrder);
                });
            }
            this._updateVisibleRows();
        }
    };
    SmdDataTable.prototype._sortRows = function (a, b, sortDir) {
        if (sortDir === void 0) { sortDir = 'asc'; }
        var dir = (sortDir == 'asc' ? 1 : -1);
        if (a > b) {
            return 1 * dir;
        }
        if (a < b) {
            return -1 * dir;
        }
        return 0;
    };
    SmdDataTable.prototype._onPageChange = function (event) {
        this.pageChange.emit(event);
        this._updateVisibleRows();
    };
    SmdDataTable.prototype._columnTemplates = function () {
        return this.columns.toArray().map(function (c) { return c.template; });
    };
    SmdDataTable.prototype.refresh = function () {
        this._updateRows();
    };
    SmdDataTable.prototype._updateVisibleRows = function () {
        var _this = this;
        if (this.paginated) {
            this.visibleRows = this.rows.filter(function (value, index) { return _this.paginatorComponent.currentPage.isInsidePage(index); });
        }
        else {
            this.visibleRows = this.rows;
        }
    };
    SmdDataTable.prototype._shouldRenderCheckbox = function () {
        return this.rows.length > 0 && this.header.shouldRenderCheckbox();
    };
    __decorate([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ViewChild"])(_smd_paginator_paginator_component__WEBPACK_IMPORTED_MODULE_1__["SmdPaginatorComponent"]),
        __metadata("design:type", _smd_paginator_paginator_component__WEBPACK_IMPORTED_MODULE_1__["SmdPaginatorComponent"])
    ], SmdDataTable.prototype, "paginatorComponent", void 0);
    __decorate([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ContentChild"])(SmdDatatableHeader),
        __metadata("design:type", SmdDatatableHeader)
    ], SmdDataTable.prototype, "header", void 0);
    __decorate([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ContentChildren"])(SmdDataTableColumnComponent),
        __metadata("design:type", _angular_core__WEBPACK_IMPORTED_MODULE_0__["QueryList"])
    ], SmdDataTable.prototype, "columns", void 0);
    __decorate([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__["Input"])(),
        __metadata("design:type", Array)
    ], SmdDataTable.prototype, "models", void 0);
    __decorate([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__["Input"])(),
        __metadata("design:type", Boolean)
    ], SmdDataTable.prototype, "checked", void 0);
    __decorate([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__["Input"])(),
        __metadata("design:type", Boolean)
    ], SmdDataTable.prototype, "paginated", void 0);
    __decorate([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__["Input"])(),
        __metadata("design:type", Array)
    ], SmdDataTable.prototype, "paginatorRanges", void 0);
    __decorate([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__["Input"])(),
        __metadata("design:type", Boolean)
    ], SmdDataTable.prototype, "responsive", void 0);
    __decorate([
        Object(_angular_core__WEBPACK_IMPORTED_MODULE_0__["Input"])(),
        __metadata("design:type", Number)
    ], SmdDataTable.prototype, "counts", void 0);
