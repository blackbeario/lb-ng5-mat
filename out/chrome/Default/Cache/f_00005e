            var pageCount = (this.count / this.selectedRange) + ((this.count % this.selectedRange) > 0 ? 1 : 0);
            return pageCount ? parseInt('' + pageCount) : 0;
        },
        enumerable: true,
        configurable: true
    });
    Object.defineProperty(SmdPaginatorComponent.prototype, "pageStart", {
        get: function () {
            return parseInt('' + ((this.selectedPage * this.selectedRange) - this.selectedRange + 1));
        },
        enumerable: true,
        configurable: true
    });
    Object.defineProperty(SmdPaginatorComponent.prototype, "pageEnd", {
