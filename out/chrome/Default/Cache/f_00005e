            return this._selectedRange;
        },
        set: function (selectedRange) {
            var current = this._selectedRange;
            this._selectedRange = selectedRange;
            if (current != this._selectedRange) {
                this.reset();
            }
        },
        enumerable: true,
        configurable: true
    });
    SmdPaginatorComponent.prototype.ngOnInit = function () {
        if (!this.selectedRange) {
            this.selectedRange = this.ranges[0];
        }
    };
    SmdPaginatorComponent.prototype.onPreviousClick = function () {
        if (this.selectedPage > 1) {
            this.selectedPage -= 1;
            this.pageChange.emit(this.currentPage);
        }
    };
    SmdPaginatorComponent.prototype.onNextClick = function () {
