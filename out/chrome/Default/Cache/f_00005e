    function RealtimeService(auth) {
        this.auth = auth;
        this.cached = [];
        this.userId = this.auth.getCurrentUserId();
        var serverURL = _api_config__WEBPACK_IMPORTED_MODULE_3__["ApiConfig"].getPath();
        this.socket = socket_io_client__WEBPACK_IMPORTED_MODULE_2__(serverURL);
    }
    RealtimeService.prototype.connect = function () {
        var _this = this;
        var accessTokenId = this.auth.getAccessTokenId();
        this.userId = this.userId;
        this.socket.emit('authentication', {
            id: accessTokenId, userId: this.userId
        });
        this.socket.on('authenticated', function () {
            console.log('socket authenticated user: ' + _this.userId);
        });
        this.socket.on("disconnect", function () {
            console.log('socket disconnected user: ' + _this.userId);
        });
        this.auth.onAuthChange$.subscribe(function (user) {
            var token = _this.auth.getAccessTokenId();
            var uid;
            if (user && user.id) {
                uid = user.id;
            }
            if (uid !== _this.userId) {
                _this.unSubscribeUser(_this.userId);
                _this.reconnect();
                _this.userId = uid;
            }
        });
    };
    RealtimeService.prototype.reconnect = function () {
        this.disconnect();
        this.connect();
    };
    RealtimeService.prototype.disconnect = function () {
        this.socket.disconnect();
    };
    RealtimeService.prototype.observable = function (options) {
