    UserService.prototype.isAdministrator = function () {
        if (this.isAuthenticated()) {
            var user = this.auth.getCurrentUserData();
            if (user && user.roles) {
                var roles = user.roles;
                if (roles.length) {
                    var adminRoles = _api_config__WEBPACK_IMPORTED_MODULE_4__["ApiConfig"].getAdminRoles();
                    for (var i = 0; i < adminRoles.length; i++) {
                        for (var j = 0; j < roles.length; j++) {
                            if (roles[j].name == adminRoles[i]) {
                                return true;
                            }
                        }
                    }
                }
            }
        }
        return false;
    };
